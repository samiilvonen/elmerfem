! Test case for ascii STL output. We removed the HeatSolver for simplicity.
! 
! P.R. 24.4.2025

Header
  CHECK KEYWORDS Warn
  Mesh DB "." "cubes"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 5

  Coordinate System = "Cartesian"

  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 0
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
End

Body 1
  Name = "BodyA"
  Equation = 1
  Material = 1
End

Body 2
  Name = "BodyB"
  Body Force = 1
  Equation = 1
  Material = 2
End

Equation 1
  Name = "Equations"
  Active Solvers(1) = 1 
End


Solver 1
  Equation = "ResultOutput"
  Procedure = "ResultOutputSolve" "ResultOutputSolver"
  Output File Name = "case" 

  Stl format = Logical True
  stl separate bodies = logical true
!  stl separate bcs = logical true
!  stl min entity = integer 2
!  stl max entity = integer 2

! "always" sets test norm to zero? 
  exec solver = after all 
End


Material 1
  Name = "MaterialA"
  Density = 1
  Heat Conductivity = 1
End

Material 2
  Name = "MaterialB"
  Density = 1
  Heat Conductivity = 2
End

Body Force 1
  Name = "BodyForce"
  Heat Source = 1
End

Boundary Condition 1
  Name = "Constraint"
  Target Boundaries(1) = 1 
  Temperature = 0
End

Boundary Condition 1
  Name = "Constraint1"
  Target Boundaries(1) = 1 
  Temperature = 0
End

Boundary Condition 2
  Name = "Constraint2"
  Target Boundaries(1) = 2
  Temperature = 0
End

Boundary Condition 3
  Name = "Constraint3"
  Target Boundaries(1) = 3 
End
Boundary Condition 4
  Name = "Constraint4"
  Target Boundaries(1) = 4 
End

Boundary Condition 5
  Name = "Constraint5"
  Target Boundaries(1) = 5
End

Boundary Condition 6
  Name = "Constraint6"
  Target Boundaries(1) = 6
End

Boundary Condition 7
  Name = "Constraint7"
  Target Boundaries(1) = 7
End

Solver 1 :: Reference Values(6) = 26 13 4563 734 24336160 2408.25
Solver 1 :: Reference Norm = 1.0
!End Of File
